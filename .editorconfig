# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[Makefile]
indent_style = tab

[lib/**.ts]
indent_style = space
indent_size = 2

[package.json]
indent_style = space
indent_size = 2

[*]
indent_style = space
indent_size = 2
end_of_line = crlf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
